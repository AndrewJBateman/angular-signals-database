{
  "name": "angular-signals-database",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "format": "prettier --write --config .prettierrc \"src/**/*.{js,ts,html,json}",
    "dev:ssr": "ng run angular-signals-database:serve-ssr",
    "serve:ssr": "node dist/angular-signals-database/server/main.js",
    "copy:files": "node ./scripts/copyFiles.js",
    "build:ssr": "ng build && ng run angular-signals-database:server:production",
    "prerender": "ng run angular-signals-database:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.2",
    "@angular/cdk": "^16.1.1",
    "@angular/common": "^16.1.2",
    "@angular/compiler": "^16.1.2",
    "@angular/core": "^16.1.2",
    "@angular/forms": "^16.1.2",
    "@angular/material": "^16.1.1",
    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/platform-server": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@netlify/functions": "^1.6.0",
    "@nguniversal/express-engine": "^16.1.0",
    "express": "^4.15.2",
    "fs-extra": "^11.1.1",
    "rxjs": "~7.8.1",
    "serverless-http": "^3.2.0",
    "tslib": "^2.5.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.2",
    "@nguniversal/builders": "^16.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.4",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "^8.40.0",
    "jasmine-core": "~5.0.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.0",
    "typescript": "~5.0.3"
  }
}
